[sub_resource type="GDScript" id="GDScript_iedsq"]
resource_name = "start_menu"
script/source = "extends Node2D

enum SelectState{
	Null,
	LoadSave,
	NewSave
}

@onready var options_container = get_node(\"Camera2D/CanvasLayer/StartMenu/StartMenuContainer\")
@onready var saves_container = get_node(\"Camera2D/CanvasLayer/StartMenu/SavesContainer\")
@onready var difficulties_container = get_node(\"Camera2D/CanvasLayer/StartMenu/DifficultiesContainer\")
@onready var difficulties_box = get_node(\"Camera2D/CanvasLayer/StartMenu/DescBox\")
@onready var difficulties_desc = get_node(\"Camera2D/CanvasLayer/StartMenu/DescBox/DifficultyDesc\")
@onready var name_edit = get_node(\"Camera2D/CanvasLayer/StartMenu/DescBox/NameEdit\")
@onready var help_box = get_node(\"Camera2D/CanvasLayer/StartMenu/HelpBox\")
@onready var help_box2 = get_node(\"Camera2D/CanvasLayer/StartMenu/HelpBox2\")

var select_state : SelectState = SelectState.Null
var selected_save : String
var player : Player
@onready var start_suit = preload(\"res://day/Resources/SuitResources/NoSuitResource.tres\")

func _ready():
	_show_options()
	player = get_node(\"Player\")
	player.set_process(false)
	player.set_physics_process(false)
	
# Main scene button logic
func _on_start_button_pressed():
	select_state = SelectState.NewSave
	GameManager.is_time_cycle_enabled = true
	_show_saves(true)

func _on_continue_button_pressed():
	pass # TODO: ADD PREVIOUS GAME SHOWS ON GAME LOAD

func _on_load_button_pressed():
	select_state = SelectState.LoadSave
	GameManager.is_time_cycle_enabled = true
	_show_saves(false)

func _on_quit_button_pressed():
	get_tree().quit()

#save container logic
func on_save_press(save_name : String):
	if select_state == SelectState.NewSave:
		selected_save = save_name
		_show_difficulties()
	elif select_state == SelectState.LoadSave:
		SaveManager.current_save_file = save_name
		player.set_process(true)
		player.set_physics_process(true)
		var save_data : SaveResource = SaveManager.load_data(save_name)
		SaveManager.current_save_resource = save_data
		save_data.apply_data()

#difficulty logic
func on_difficulty_lock(title : String, desc : String):
	difficulties_desc.text = title + \"\\n\" + desc

func on_new_game_start():
	player.set_process(true)
	player.set_physics_process(true)
	player._set_suit(start_suit)
	SaveManager.current_save_file = selected_save
	SaveManager.current_save_resource = SaveResource.new()
	if name_edit.text == \"\":
		SaveManager.current_save_resource.player_name = \"SUKI\"
	else:
		SaveManager.current_save_resource.player_name = name_edit.text
	GameManager._change_scene(\"HomeScene\")
	
#Remaining logic
func _show_saves(enable_empty : bool = true):
	options_container.hide()
	saves_container.show()
	difficulties_container.hide()
	difficulties_box.hide()
	for child in saves_container.get_children():
		if child.name != \"BackButton\":
			child.set_up(enable_empty)
	
func _show_options():
	help_box.hide()
	help_box2.hide()
	options_container.show()
	saves_container.hide()
	difficulties_container.hide()
	difficulties_box.hide()

func _show_difficulties():
	help_box.hide()
	help_box2.hide()
	options_container.hide()
	saves_container.hide()
	difficulties_container.show()
	difficulties_box.show()
	for child in difficulties_container.get_children():
		if child.default_difficulty:
			child._on_difficulty_press()
			return


func _on_help_button_pressed() -> void:
	print(\"help\") # Replace with function body.
	options_container.hide()
	saves_container.hide()
	difficulties_container.hide()
	help_box.show()
	help_box2.show()
"

[sub_resource type="GDScript" id="GDScript_t5kqk"]
script/source = "extends Button

@export var save_name : String

@onready var start_manager = get_tree().root.get_child(GameManager.AUTOLOAD_AMOUNT)

var has_data = false

func _ready():
	if !save_name.ends_with(\".json\"):
		save_name += \".json\"
	has_data = SaveManager.check_save_exist(save_name)
	if has_data:
		var data = SaveManager.load_data(save_name)
		text = data.player_name

func set_up(show_all : bool):
	if show_all:
		disabled = false
	else:
		disabled = !has_data

func _on_save_button_press():
	start_manager.on_save_press(save_name)
"

[sub_resource type="GDScript" id="GDScript_0k21y"]
script/source = "extends Button

@onready var manager = get_tree().root.get_child(GameManager.AUTOLOAD_AMOUNT)

var title : String
@export_multiline var desc : String
@export var default_difficulty : bool

func _ready():
	title = text

func _on_difficulty_press():
	manager.on_difficulty_lock(title, desc)
"

